<EditForm Model="@_editUserModel" OnValidSubmit="SubmitAsync">
    <MudDialog>
        <TitleContent>
            @{
                <MudText Typo="Typo.h6">
                    <MudIcon Icon="@Icons.Material.Filled.VerifiedUser" Class="mr-3 mb-n1" />
                    @Localizer["Modifier Utilisateur"]
                </MudText>
            }
        </TitleContent>
        <DialogContent>
         <MudTextField @bind-Value="@UserName" Disabled="true" />
         <MudTextField @bind-Value="@Email" Disabled="true" />
            
            

        </DialogContent>
        <DialogActions>
            <MudButton Variant="Variant.Filled" OnClick="Cancel">@Localizer["Annuler"]</MudButton>
            @{
                <MudButton Variant="Variant.Filled" ButtonType="ButtonType.Submit"  Color="Color.Success">@Localizer["Enregistrer"]</MudButton>
            }
        </DialogActions>
    </MudDialog>
</EditForm>